{
  "main": "./lib/keytar.js",
  "typings": "keytar.d.ts",
  "name": "keytar-forked-forked",
  "description": "Bindings to native Mac/Linux/Windows password APIs (forked)",
  "version": "7.10.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devm33/node-keytar.git"
  },
  "bugs": {
    "url": "https://github.com/devm33/node-keytar/issues"
  },
  "homepage": "https://github.com/devm33/node-keytar",
  "keywords": [
    "keychain",
    "password",
    "passwords",
    "credential",
    "credentials",
    "vault",
    "credential vault"
  ],
  "files": [
    "lib",
    "src",
    "script",
    "binding.gyp",
    "keytar.d.ts",
    "prebuilds"
  ],
  "types": "./keytar.d.ts",
  "scripts": {
    "install": "node script/install.js || npm run build",
    "build": "node-gyp rebuild",
    "lint": "npm run cpplint",
    "cpplint": "node-cpplint --filters legal-copyright,build-include,build-namespaces src/*.cc",
    "test": "npm run lint && npm rebuild && mocha spec/",
    "prebuild-napi-x64": "prebuild -t 3 -r napi -a x64 --strip",
    "prebuild-napi-ia32": "prebuild -t 3 -r napi -a ia32 --strip",
    "prebuild-napi-arm64": "prebuild -t 3 -r napi -a arm64 --strip",
    "prebuild-napi-armv7l": "prebuild -t 3 -r napi -a armv7l --strip",
    "prebuild-napi-arm": "prebuild -t 3 -r napi -a arm --strip"
  },
  "devDependencies": {
    "chai": "^5.1.2",
    "mocha": "^11.1.0",
    "node-cpplint": "~0.4.0",
    "node-gyp": "^11.0.0",
    "prebuild": "^13.0.1"
  },
  "dependencies": {
    "node-addon-api": "^8.3.0"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "config": {
    "runtime": "napi",
    "target": 3
  }
}
